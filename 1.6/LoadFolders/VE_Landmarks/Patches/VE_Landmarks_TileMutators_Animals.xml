<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- ==========[VEE_NaturalAerie]==========-->
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/TileMutatorDef[defName="VEE_NaturalAerie"]/biomeBlacklist</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/TileMutatorDef[defName="VEE_NaturalAerie"]</xpath>
			<value>
				<biomeBlacklist/>
			</value>
		</nomatch>
	</Operation>
	<!-- ==========[VEE_Alphabeavers]==========-->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/TileMutatorDef[defName="VEE_Alphabeavers"]/biomeBlacklist</xpath>
		<value>
			<!-- Blighted -->
			<li>Mashed_Ashlands_BlightedAshlands</li>
			<li>Mashed_Ashlands_BlightedBogAshlands</li>
			<li>Mashed_Ashlands_BlightedPetrifiedForestAshlands</li>
			<!-- Regular -->
			<li>Mashed_Ashlands_Ashlands</li>
			<li>Mashed_Ashlands_DustplainAshlands</li>
			<li>Mashed_Ashlands_GrazelandAshlands</li>
			<li>Mashed_Ashlands_HighlandAshlands</li>
			<!-- Volcanic -->
			<li>Mashed_Ashlands_VolcanicAshlands</li>
			<li>Mashed_Ashlands_VolcanicBadlandAshlands</li>
			<li>Mashed_Ashlands_VolcanicBasaltHillsAshlands</li>
			<li>Mashed_Ashlands_VolcanicSulphurPitsAshlands</li>
		</value>
	</Operation>
	<!-- ==========[VEE_MigratoryHerds]==========-->
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/TileMutatorDef[defName="VEE_MigratoryHerds"]/biomeBlacklist</xpath>
		<value>
			<!-- Blighted -->
			<li>Mashed_Ashlands_BlightedAshlands</li>
			<li>Mashed_Ashlands_BlightedBogAshlands</li>
			<li>Mashed_Ashlands_BlightedPetrifiedForestAshlands</li>
			<!-- Regular -->
			<li>Mashed_Ashlands_DustplainAshlands</li>
			<!-- Volcanic -->
			<li>Mashed_Ashlands_VolcanicAshlands</li>
			<li>Mashed_Ashlands_VolcanicBadlandAshlands</li>
			<li>Mashed_Ashlands_VolcanicBasaltHillsAshlands</li>
			<li>Mashed_Ashlands_VolcanicSulphurPitsAshlands</li>
		</value>
	</Operation>
</Patch>
