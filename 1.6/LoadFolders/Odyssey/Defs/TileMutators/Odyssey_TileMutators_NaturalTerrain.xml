<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ==========[Mashed_Ashlands_BasePatch]==========-->
	<TileMutatorDef ParentName="BasePatch" Name="Mashed_Ashlands_BasePatch" Abstract="true">
		<categories>
			<li>Mashed_Ashlands_TerrainPatch</li>
		</categories>
		<chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_AshMuddy]==========-->
	<TileMutatorDef ParentName="Mashed_Ashlands_BasePatch">
		<defName>Mashed_Ashlands_AshMuddy</defName>
		<label>muddy ash</label>
		<description>Large patches of muddy ash are scattered throughout this area.</description>
		<biomeWhitelist>
			<!-- Regular -->
			<li>Mashed_Ashlands_Ashlands</li>
			<li>Mashed_Ashlands_GrazelandAshlands</li>
			<li>Mashed_Ashlands_SwampAshlands</li>
			<!-- Thriving -->
			<li>Mashed_Ashlands_ForestAshlands</li>
			<li>Mashed_Ashlands_MarshAshlands</li>
			<li>Mashed_Ashlands_MeadowAshlands</li>
		</biomeWhitelist>
		<terrainPatchMakers>
			<li>
				<perlinFrequency>0.035</perlinFrequency>
				<thresholds>
					<li>
						<terrain>Mashed_Ashlands_AshMud</terrain>
						<min>0.64</min>
						<max>999</max>
					</li>
				</thresholds>
			</li>
		</terrainPatchMakers>
	</TileMutatorDef>
