<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ==========[Mashed_Ashlands_AshPlantGrove]==========-->
	<TileMutatorDef>
		<defName>Mashed_Ashlands_AshPlantGrove</defName>
		<label>ash plant grove</label>
		<description>This area contains a large number of {0}.</description>
		<workerClass>TileMutatorWorker_PlantGrove</workerClass>
		<categories>
			<li>WildPlants</li>
		</categories>
		<chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
		<plantDensityRange>0.3~4</plantDensityRange>
		<pollutionRange>0~0.25</pollutionRange>
		<plantKinds>
			<li>Mashed_Ashlands_Plant_AshYam</li>
			<li>Mashed_Ashlands_Plant_Corkbulb</li>
			<li>Mashed_Ashlands_Plant_TramaShrub</li>
			<li>Mashed_Ashlands_Plant_Scathecraw</li>
			<li>Mashed_Ashlands_Plant_AshPomegranate</li>
			<li>Mashed_Ashlands_Plant_TreeEmperorParasol</li>
			<li>Mashed_Ashlands_Plant_TreeNetchShield</li>
			<li>Mashed_Ashlands_Plant_TreeWeepingLeathercap</li>
		</plantKinds>
		<biomeWhitelist>
			<!-- Regular -->
			<li>Mashed_Ashlands_Ashlands</li>
			<li>Mashed_Ashlands_DustplainAshlands</li>
			<li>Mashed_Ashlands_GrazelandAshlands</li>
			<li>Mashed_Ashlands_HighlandAshlands</li>
			<li>Mashed_Ashlands_SwampAshlands</li>
			<!-- Thriving -->
			<li>Mashed_Ashlands_ForestAshlands</li>
			<li>Mashed_Ashlands_MarshAshlands</li>
			<li>Mashed_Ashlands_MeadowAshlands</li>
			<!-- Volcanic -->
			<li>Mashed_Ashlands_VolcanicAshlands</li>
			<li>Mashed_Ashlands_VolcanicBadlandAshlands</li>
			<li>Mashed_Ashlands_VolcanicBasaltHillsAshlands</li>
			<li>Mashed_Ashlands_VolcanicSulphurPitsAshlands</li>
		</biomeWhitelist>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_AshyMutator]==========-->
	<TileMutatorDef ParentName="Mashed_Ashlands_AshlandMutatorBase">
		<defName>Mashed_Ashlands_AshyMutator</defName>
		<label>ashy</label>
		<description>The breeze constantly releases ash over this region. Ash fall is much more common here.</description>
		<categories>
			<li>Weather</li>
		</categories>
		<workerClass>TileMutatorWorker_IncreaseWeatherFrequency</workerClass>
		<chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
		<weathersToAffect>
			<li>Mashed_Ashlands_Ashfall</li>
		</weathersToAffect>
		<weatherFrequencyOffset>10</weatherFrequencyOffset>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_DustAntHives_Increased]==========-->
	<TileMutatorDef>
		<defName>Mashed_Ashlands_DustAntHives_Increased</defName>
		<label>increased dust ants</label>
		<description>This area has more dust ant hives than normal.</description>
		<chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
		<extraGenSteps>
			<li>Mashed_Ashlands_DustAntHives_Increased</li>
		</extraGenSteps>
		<biomeWhitelist>
			<li>Mashed_Ashlands_DustplainAshlands</li>
		</biomeWhitelist>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_FetcherflyHives_Increased]==========-->
	<TileMutatorDef>
		<defName>Mashed_Ashlands_FetcherflyHives_Increased</defName>
		<label>increased fetcherfly</label>
		<description>This area has more fetcherfly hives than normal.</description>
		<chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
		<extraGenSteps>
			<li>Mashed_Ashlands_FetcherflyHives_Increased</li>
		</extraGenSteps>
		<biomeWhitelist>
			<!-- Blighted -->
			<li>Mashed_Ashlands_BlightedAshlands</li>
			<li>Mashed_Ashlands_BlightedBogAshlands</li>
			<li>Mashed_Ashlands_BlightedPetrifiedForestAshlands</li>
			<!-- Regular -->
			<li>Mashed_Ashlands_Ashlands</li>
			<!-- Volcanic -->
			<li>Mashed_Ashlands_VolcanicAshlands</li>
			<li>Mashed_Ashlands_VolcanicBadlandAshlands</li>
			<li>Mashed_Ashlands_VolcanicBasaltHillsAshlands</li>
			<li>Mashed_Ashlands_VolcanicSulphurPitsAshlands</li>
		</biomeWhitelist>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_StriderShells_Increased]==========-->
	<TileMutatorDef>
		<defName>Mashed_Ashlands_StriderShells_Increased</defName>
		<label>strider graveyard</label>
		<description>This area is a silt strider graveyard. There are many dessicated silt strider shells.</description>
		<chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
		<extraGenSteps>
			<li>Mashed_Ashlands_StriderShells_Increased</li>
		</extraGenSteps>
		<biomeWhitelist>
			<!-- Blighted -->
			<li>Mashed_Ashlands_BlightedAshlands</li>
			<li>Mashed_Ashlands_BlightedBogAshlands</li>
			<li>Mashed_Ashlands_BlightedPetrifiedForestAshlands</li>
			<!-- Volcanic -->
			<li>Mashed_Ashlands_VolcanicBadlandAshlands</li>
		</biomeWhitelist>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_DustyMutator]==========-->
	<TileMutatorDef>
		<defName>Mashed_Ashlands_DustyMutator</defName>
		<label>dusty</label>
		<description>There's so much dust here that even the faintest breeze can whip up a dust storm. Dust storms are much more common here.</description>
		<categories>
			<li>Weather</li>
		</categories>
		<workerClass>TileMutatorWorker_IncreaseWeatherFrequency</workerClass>
		<chanceOnNonLandmarkTile>0.02</chanceOnNonLandmarkTile>
		<weathersToAffect>
			<li>Mashed_Ashlands_DustStorm</li>
		</weathersToAffect>
		<weatherFrequencyOffset>10</weatherFrequencyOffset>
		<biomeWhitelist>
			<li>Mashed_Ashlands_DustplainAshlands</li>
		</biomeWhitelist>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_DusttideMutator]==========-->
	<TileMutatorDef>
		<defName>Mashed_Ashlands_DusttideMutator</defName>
		<label>dusttide</label>
		<description>Copious amounts of dust and metallic particulate ensure even the slightest breeze results in an electrified dust storm. Dust thunderstorms are much more common here.</description>
		<categories>
			<li>Weather</li>
		</categories>
		<workerClass>TileMutatorWorker_IncreaseWeatherFrequency</workerClass>
		<chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
		<weathersToAffect>
			<li>Mashed_Ashlands_DustThunderstorm</li>
		</weathersToAffect>
		<weatherFrequencyOffset>10</weatherFrequencyOffset>
		<biomeWhitelist>
			<li>Mashed_Ashlands_DustplainAshlands</li>
		</biomeWhitelist>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_SulphuricAtmosphereMutator]==========-->
	<TileMutatorDef>
		<defName>Mashed_Ashlands_SulphuricAtmosphereMutator</defName>
		<label>sulphuric atmosphere</label>
		<description>Frequent volcanic activity ensures the atmosphere here has plenty of sulphuric dioxide in it. sulphuric rain is much more common here</description>
		<categories>
			<li>Weather</li>
		</categories>
		<workerClass>TileMutatorWorker_IncreaseWeatherFrequency</workerClass>
		<chanceOnNonLandmarkTile>0.01</chanceOnNonLandmarkTile>
		<weathersToAffect>
			<li>Mashed_Ashlands_SulphuricRain</li>
		</weathersToAffect>
		<weatherFrequencyOffset>10</weatherFrequencyOffset>
		<biomeWhitelist>
			<li>Mashed_Ashlands_VolcanicAshlands</li>
			<li>Mashed_Ashlands_VolcanicBadlandAshlands</li>
			<li>Mashed_Ashlands_VolcanicBasaltHillsAshlands</li>
			<li>Mashed_Ashlands_VolcanicSulphurPitsAshlands</li>
		</biomeWhitelist>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_EbonyDeposits]==========-->
	<TileMutatorDef ParentName="Mashed_Ashlands_AshlandMutatorBase">
		<defName>Mashed_Ashlands_EbonyDeposits</defName>
		<label>volcanic ebony deposits</label>
		<description>This area has deposits of volcanic ebony, which can be mined for ebony glass.</description>
		<chanceOnNonLandmarkTile>0.03</chanceOnNonLandmarkTile>
		<workerClass>Mashed_Ashlands.TileMutatorWorker_EbonyDeposits</workerClass>
	</TileMutatorDef>
	<!-- ==========[Mashed_Ashlands_GlassDeposits]==========-->
	<TileMutatorDef ParentName="Mashed_Ashlands_AshlandMutatorBase">
		<defName>Mashed_Ashlands_GlassDeposits</defName>
		<label>volcanic malachite deposits</label>
		<description>This area has deposits of volcanic malachite, which can be mined for malachite glass.</description>
		<chanceOnNonLandmarkTile>0.03</chanceOnNonLandmarkTile>
		<workerClass>Mashed_Ashlands.TileMutatorWorker_GlassDeposits</workerClass>
	</TileMutatorDef>
</Defs>