<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Fantasy Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="VR_BaseConstruct"]/statBases</xpath>
					<value>
						<Mashed_Ashlands_AshResistance>1</Mashed_Ashlands_AshResistance>
					</value>
				</li>
				<li Class="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[@Name="VR_BaseConstruct"]</xpath>
					<value>
						<li Class="Mashed_Ashlands.AshResistanceProperties">
							<treatAsMechanical>true</treatAsMechanical>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
