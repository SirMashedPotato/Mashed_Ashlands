	<TerrainDef ParentName="WaterDeepBase">
		<defName>Mashed_Ashlands_DeepVolcanicWater</defName>
		<label>deep volcanic water</label>
		<renderPrecedence>395</renderPrecedence>
		<texturePath>Terrain/Surfaces/Water/Mashed_Ashlands_DeepVolcanicWaterRamp</texturePath>
		<pollutedTexturePath>Terrain/Surfaces/Water/Mashed_Ashlands_DeepVolcanicWaterRampPolluted</pollutedTexturePath>
