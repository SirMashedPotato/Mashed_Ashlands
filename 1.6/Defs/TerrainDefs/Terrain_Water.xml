	<TerrainDef ParentName="Mashed_Ashlands_LiquidMudBase" Name="Mashed_Ashlands_BlightedSludgeBase">
		<defName>Mashed_Ashlands_BlightedSludge</defName>
		<label>blighted sludge</label>
		<texturePath>Terrain/Surfaces/Water/Mashed_Ashlands_BlightedSludgeRamp</texturePath>
		<driesTo>Mashed_Ashlands_AshSoil</driesTo>
		<floodTerrain MayRequire="Ludeon.RimWorld.Odyssey">Mashed_Ashlands_BlightedSludgeFloodwater</floodTerrain>
		<traversedThought>Mashed_Ashlands_WadedThroughSludge</traversedThought>
		<tags>
			<li>Mashed_Ashlands_WaterBlightedSludge</li>
		</tags>
	</TerrainDef>
	<!-- ==========[Mashed_Ashlands_ThermalMud]==========-->
	<TerrainDef ParentName="Mashed_Ashlands_LiquidMudBase" Name="Mashed_Ashlands_ThermalMudBase">
