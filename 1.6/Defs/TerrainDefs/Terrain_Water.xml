<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_LiquidMudBase]==========-->
	<TerrainDef ParentName="WaterShallowBase" Name="Mashed_Ashlands_LiquidMudBase" Abstract="true">
		<pollutionShaderType MayRequire="Ludeon.RimWorld.Biotech">TerrainWaterPolluted</pollutionShaderType>
		<waterDepthShader>Map/WaterDepth</waterDepthShader>
		<pathCost>40</pathCost>
		<renderPrecedence>394</renderPrecedence>
		<waterBodyType>Other</waterBodyType>
		<canFreeze>false</canFreeze>
		<forcePassableByFlyingPawns>true</forcePassableByFlyingPawns>
	</TerrainDef>
