<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- ==========[Mashed_Ashlands_MudcrabMerchantMeeting]==========-->
	<TraderKindDef>
		<defName>Mashed_Ashlands_MudcrabMerchant</defName>
		<label>mudcrab merchant</label>
		<stockGenerators>
			<!-- Resources -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>10000</countRange>
			</li>
			<!-- Drugs -->
			<li Class="StockGenerator_Tag">
				<tradeTag>Mashed_Ashlands_AshlandsAlcohol</tradeTag>
				<thingDefCountRange>3~6</thingDefCountRange>
				<totalPriceRange>300~600</totalPriceRange>
			</li>
			<!-- Misc -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Ashlands_AshlandJournal</thingDef>
				<countRange>1</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="thelastbulletbender.rwexploration">
				<thingDef>Mashed_Ashlands_VolcanoMap</thingDef>
				<countRange>1</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Ashlands_CephalopodHelmet</thingDef>
				<countRange>1</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Ashlands_FetcherflyHiveCore</thingDef>
				<countRange>1</countRange>
			</li>
			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple"/>
			<li Class="Mashed_Ashlands.StockGenerator_BuyExpensive"/>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>Drugs</tag>
			</li>
			<li Class="StockGenerator_Animals">
				<tradeTagsBuy>
					<li>Mashed_Ashlands_AnimalCrab</li>
				</tradeTagsBuy>
			</li>
		</stockGenerators>
	</TraderKindDef>
	<!-- ==========[Mashed_Ashlands_ScribMerchant]==========-->
	<TraderKindDef>
		<defName>Mashed_Ashlands_ScribMerchant</defName>
		<label>scrib merchant</label>
		<stockGenerators>
			<!-- Resources -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>30</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Ashlands_KwamaEgg</thingDef>
				<countRange>600</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Ashlands_VolcanicEbony</thingDef>
				<countRange>300</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Ashlands_VolcanicGlass</thingDef>
				<countRange>300</countRange>
			</li>
			<!-- Misc -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mashed_Ashlands_AshlandJournal</thingDef>
				<countRange>1</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="thelastbulletbender.rwexploration">
				<thingDef>Mashed_Ashlands_VolcanoMap</thingDef>
				<countRange>1</countRange>
			</li>
			<!-- Buying -->
			<li Class="StockGenerator_BuyExpensiveSimple"/>
			<li Class="StockGenerator_BuyTradeTag">
				<tag>Drugs</tag>
			</li>
			<li Class="StockGenerator_Animals">
				<tradeTagsSell>
					<li>Mashed_Ashlands_AnimalKwamaWorker</li>
				</tradeTagsSell>
				<tradeTagsBuy>
					<li>Mashed_Ashlands_AnimalKwama</li>
				</tradeTagsBuy>
				<kindCountRange>1</kindCountRange>
				<countRange>3</countRange>
			</li>
		</stockGenerators>
	</TraderKindDef>
</Defs>
