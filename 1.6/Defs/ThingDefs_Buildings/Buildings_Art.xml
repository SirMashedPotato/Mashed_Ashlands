<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_AshCastle]==========-->
	<ThingDef ParentName="BuildingBase">
		<defName>Mashed_Ashlands_AshCastle</defName>
		<label>ashcano</label>
		<description>A pile of ash, shaped into a small volcano. People make these for the fun of it.</description>
		<thingClass>Building</thingClass>
		<altitudeLayer>Building</altitudeLayer>
		<passability>PassThroughOnly</passability>
		<pathCost>30</pathCost>
		<pathCostIgnoreRepeat>false</pathCostIgnoreRepeat>
		<useHitPoints>true</useHitPoints>
		<graphicData>
			<graphicClass>Graphic_Random</graphicClass>
			<texPath>Things/Building/Art/Mashed_Ashlands_AshCastle</texPath>
		</graphicData>
		<fillPercent>0.35</fillPercent>
		<rotatable>false</rotatable>
		<tickerType>Rare</tickerType>
		<drawDamagedOverlay>false</drawDamagedOverlay>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Beauty>4</Beauty>
			<WorkToBuild>1000</WorkToBuild>
			<MarketValue>0</MarketValue>
		</statBases>
		<comps>
			<li>
				<compClass>Mashed_Ashlands.Comp_AshCastleCollapser</compClass>
			</li>
		</comps>
		<forceDebugSpawnable>true</forceDebugSpawnable>
		<building>
			<expandHomeArea>false</expandHomeArea>
			<repairable>false</repairable>
			<isInert>true</isInert>
			<artificialForMeditationPurposes>false</artificialForMeditationPurposes>
		</building>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_KwamaEggSculptureSmall]==========-->
	<ThingDef ParentName="SculptureBase">
		<defName>Mashed_Ashlands_KwamaEggSculptureSmall</defName>
		<label>small kwama egg sculpture</label>
		<description>A kwama egg, on a pedestal, because that makes a really great piece of art. Hopefully the egg has been preserved properly, because if it hasn't...</description>
		<graphicData>
			<graphicClass>Graphic_Random</graphicClass>
			<texPath>Things/Building/Art/Mashed_Ashlands_KwamaEggSculptureSmall</texPath>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<fillPercent>0.35</fillPercent>
		<rotatable>false</rotatable>
		<tickerType>Rare</tickerType>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<Mass>3</Mass>
			<Beauty>35</Beauty>
			<WorkToMake>12000</WorkToMake>
			<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
		</statBases>
		<researchPrerequisites>
			<li>Mashed_Ashlands_AshSurvival_Beginner</li>
		</researchPrerequisites>
		<costList>
			<Mashed_Ashlands_KwamaEgg>1</Mashed_Ashlands_KwamaEgg>
		</costList>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>30</costStuffCount>
		<comps>
			<li>
				<compClass>Mashed_Ashlands.Comp_RotstinkEmitter</compClass>
			</li>
		</comps>
	</ThingDef>
</Defs>
