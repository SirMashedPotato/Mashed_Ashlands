<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alien | Rimworld</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="XMT_Larva" or defName="XMT_Droplet" or @Name="XMT_Starbeast_Base" or defName="XMT_Base_Pod"]/statBases</xpath>
					<value>
						<Mashed_Ashlands_AshResistance>1</Mashed_Ashlands_AshResistance>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
