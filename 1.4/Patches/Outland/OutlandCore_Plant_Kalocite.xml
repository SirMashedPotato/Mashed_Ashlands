<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Outland - Core</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Outland_KalociteFormation"]/statBases</xpath>
					<value>
						<MorrowRim_AshResistance>1</MorrowRim_AshResistance>
					</value>
				</li>
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Biotech</li>
					</mods>
					<match Class="PatchOperationAdd">
						<xpath>Defs/ThingDef[defName="Outland_ToxociteFormation"]/statBases</xpath>
						<value>
							<MorrowRim_AshResistance>1</MorrowRim_AshResistance>
						</value>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
