<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Races Expanded - Sanguophage</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/XenotypeDef[defName="VRE_Strigoi" or defName="VRE_Ekkimian" or defName="VRE_Bruxa"]/genes</xpath>
					<value>
						<li>Mashed_Ashlands_AshResistance_Total</li>
					</value>
				</li>
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Alpha Genes</li>
					</mods>
					<match Class="PatchOperationAdd">
						<xpath>Defs/XenotypeDef[defName="AG_Malachai"]/genes</xpath>
						<value>
							<li>Mashed_Ashlands_AshResistance_Total</li>
						</value>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

