	<!-- ==========[Mashed_Ashlands_NixHoundGlowing]========== -->
	<ThingDef ParentName="Mashed_Ashlands_NixHoundThingBase">
		<defName>Mashed_Ashlands_NixHoundGlowing</defName>
		<label>glowing nix-hound</label>
		<description>A subspecies of nix-hound that has adapted to life in polluted regions. They are able to absorb nutrients from environmental pollution through modified ossicles, allowing them to grow far larger than the common nix-hound. Heavy chitin plates along their back, and their increased size, easily make them an apex predator.\n\n&lt;color=#E5E54C&gt;Draining Bite&lt;/color&gt;\nStabs from a nix-hounds proboscis do little damage, but will cause additional blood loss, nourishing the nix-hound in the process.\n\n&lt;color=#E5E54C&gt;Pollution Absorption&lt;/color&gt;\nGlowing nix-hound can absorb nutrients from environmental pollution, decreasing their hunger fall rate.</description>
		<modExtensions Inherit="False">
			<li Class="Mashed_Ashlands.PollutionProperties">
				<alternativePollutionStimulis>Mashed_Ashlands_PollutionAbsorption</alternativePollutionStimulis>
			</li>
			<li MayRequire="dankpyon.medieval.overhaul" Class="DankPyon.ButcherProperties">
				<hasBone>false</hasBone>
				<hasFat>true</hasFat>
			</li>
		</modExtensions>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>Mashed_Ashlands_Egg_NixHoundGlowingFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>5</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>1</eggCountRange>
			</li>
		</comps>
		<statBases>
			<MarketValue>500</MarketValue>
			<ArmorRating_Blunt>0.15</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.4</ArmorRating_Sharp>
		</statBases>
		<tools Inherit="False">
			<li>
				<label>left claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>14</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<chanceFactor>0.5</chanceFactor>
			</li>
			<li>
				<label>right claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>14</power>
				<cooldownTime>1.5</cooldownTime>
				<linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<chanceFactor>0.5</chanceFactor>
			</li>
			<li>
				<label>proboscis</label>
				<capacities>
					<li>Mashed_Ashlands_DrainingStab</li>
				</capacities>
				<power>13</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
				<chanceFactor>0.9</chanceFactor>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<race>
			<baseBodySize>1.4</baseBodySize>
			<baseHungerRate>0.25</baseHungerRate>
			<baseHealthScale>1.4</baseHealthScale>
			<maxPreyBodySize>2.8</maxPreyBodySize>
			<manhunterOnTameFailChance>0.3</manhunterOnTameFailChance>
			<manhunterOnDamageChance>1</manhunterOnDamageChance>
			<fleshType>Mashed_Ashlands_FleshGlowing</fleshType>
			<bloodDef>Mashed_Ashlands_Filth_GlowingBlood</bloodDef>
		</race>
	</ThingDef>
