	<!-- ==========[Mashed_Ashlands_Egg_NetchGlowingFertilized]==========-->
	<ThingDef ParentName="EggFertBase">
		<defName>Mashed_Ashlands_Egg_NetchGlowingFertilized</defName>
		<label>glowing netch egg (fert.)</label>
		<description>A fertilised glowing netch egg. If all goes well, it should hatch into a glowing netch calf. It can be eaten raw, but it's much better cooked. As glowing netch are rather protective of their eggs, glowing netch eggs somewhat valuable.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/Mashed_Ashlands_JellyEgg</texPath>
			<color>(125,173,102)</color>
		</graphicData>
		<statBases>
			<MarketValue>40</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Hatcher">
				<hatcherDaystoHatch>3</hatcherDaystoHatch>
				<hatcherPawn>Mashed_Ashlands_NetchGlowing</hatcherPawn>
			</li>
		</comps>
	</ThingDef>
