<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- ==========[Crafting wood logs]==========-->
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName = "Mashed_Ashlands_MakeWoodFromCorkbulbBase"]/products/WoodLog</xpath>
					<value>
						<DankPyon_RawWood>5</DankPyon_RawWood>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>Defs/RecipeDef[defName = "Mashed_Ashlands_MakeWoodFromCorkbulbBulk"]/products/WoodLog</xpath>
					<value>
						<DankPyon_RawWood>20</DankPyon_RawWood>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>

