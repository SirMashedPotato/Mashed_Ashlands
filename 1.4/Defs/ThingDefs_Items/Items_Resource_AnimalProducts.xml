<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[MorrowRim_OrganicProductBase]==========-->
	<ThingDef ParentName="OrganicProductBase" Name="MorrowRim_OrganicProductBase" Abstract="True">
		<socialPropernessMatters>true</socialPropernessMatters>
		<possessionCount>10</possessionCount>
		<ingestible>
			<foodType>AnimalProduct</foodType>
		</ingestible>
		<thingCategories>
			<li>AnimalProductRaw</li>
		</thingCategories>
		<tradeTags>
			<li>MorrowRim_AshlandsAnimalProduct</li>
		</tradeTags>
	</ThingDef>
	<!-- ==========[MorrowRim_KwamaEgg]==========-->
	<ThingDef ParentName="MorrowRim_OrganicProductBase">
		<defName>MorrowRim_KwamaEgg</defName>
		<label>kwama egg</label>
		<description>An infertile kwama egg, gathered from a kwama egg mine. While the idea of eating an insect egg the size of a small child may seem strange to some people, kwama eggs are considered to be quite the delicacy.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/MorrowRim_KwamaEgg</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<stackLimit>25</stackLimit>
		<statBases>
			<MarketValue>5</MarketValue>
			<Mass>0.5</Mass>
			<Nutrition>0.08</Nutrition>
			<FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>90</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>RawBad</preferability>
		</ingestible>
	</ThingDef>
	<!-- ==========[MorrowRim_NetchJelly]==========-->
	<ThingDef Name="MorrowRim_NetchJellyBase" ParentName="MorrowRim_OrganicProductBase">
		<defName>MorrowRim_NetchJelly</defName>
		<label>netch jelly</label>
		<description>A jelly like substance 'milked' from a netch. While it can be eaten raw, it's not quite as safe as milk. Often used as baby food for people that settle the ashlands, as the nematocysts found in it help with teething pain.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/MorrowRim_NetchJelly</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>1.9</MarketValue>
			<Mass>0.045</Mass>
			<MaxHitPoints>60</MaxHitPoints>
			<Flammability>0.2</Flammability>
			<Nutrition>0.05</Nutrition>
			<FoodPoisonChanceFixedHuman>0.05</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>8</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>RawTasty</preferability>
			<foodType>AnimalProduct, Fluid</foodType>
			<babiesCanIngest>true</babiesCanIngest>
		</ingestible>
	</ThingDef>
	<!-- ==========[MorrowRim_NixOxMilk]==========-->
	<ThingDef ParentName="MorrowRim_OrganicProductBase">
		<defName>MorrowRim_NixOxMilk</defName>
		<label>nix-ox mucous</label>
		<description>A viscous liquid secreted by a nix-ox. Rather unpleasant when consumed raw, though surprisingly tasty when cooked. Some have even figured out how to refine it into chemfuel.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/MorrowRim_NixOxMilk</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>1</MarketValue>
			<Mass>0.045</Mass>
			<MaxHitPoints>60</MaxHitPoints>
			<Flammability>0.2</Flammability>
			<Nutrition>0.05</Nutrition>
			<FoodPoisonChanceFixedHuman>0.05</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>16</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>RawBad</preferability>
			<foodType>AnimalProduct, Fluid</foodType>
			<specialThoughtDirect>MorrowRim_AteNixOxMilk</specialThoughtDirect>
		</ingestible>
	</ThingDef>
	<!-- ==========[MorrowRim_ScribJelly]==========-->
	<ThingDef ParentName="MorrowRim_OrganicProductBase">
		<defName>MorrowRim_ScribJelly</defName>
		<label>scrib jelly</label>
		<description>A dead scrib, crushed into a jelly-like substance. Has a noticeable sour taste, but is very nutritious. Often consumed raw.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/MorrowRim_ScribJelly</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>1.9</MarketValue>
			<Mass>0.045</Mass>
			<MaxHitPoints>60</MaxHitPoints>
			<Flammability>0.2</Flammability>
			<Nutrition>0.05</Nutrition>
			<FoodPoisonChanceFixedHuman>0.02</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>10</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>RawTasty</preferability>
			<foodType>AnimalProduct, Fluid</foodType>
			<babiesCanIngest>true</babiesCanIngest>
		</ingestible>
	</ThingDef>
	<!-- ==========[MorrowRim_SwampJellyEdible]==========-->
	<ThingDef ParentName="MorrowRim_NetchJellyBase">
		<defName>MorrowRim_SwampJellyEdible</defName>
		<label>swamp jelly</label>
		<description>A jelly like substance harvested from a swamp jelly. While it can be eaten raw, it's not quite as safe as milk. Often used as baby food for people that settle the ashlands, as the nematocysts found in it help with teething pain.</description>
		<graphicData>
			<texPath>Things/Item/Resource/AnimalProductRaw/MorrowRim_SwampJelly</texPath>
		</graphicData>
	</ThingDef>
</Defs>
