	<ThingDef ParentName="ResourceBase" Name="Mashed_Ashlands_AlcoholPrecursorBase" Abstract="True" MayRequireAnyOf="oskarpotocki.vanillafactionsexpanded.core, syrchalis.processor.framework">
		<graphicData>
