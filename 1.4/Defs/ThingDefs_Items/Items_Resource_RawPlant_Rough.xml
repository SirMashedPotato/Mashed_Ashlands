<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_PlantRoughBase]==========-->
	<ThingDef ParentName="RoughPlantBase" Name="Mashed_Ashlands_PlantRoughBase" Abstract="True">
		<possessionCount>10</possessionCount>
		<tradeTags>
			<li>Mashed_Ashlands_AshlandsRawPlant</li>
			<li>Mashed_Ashlands_AshlandsRawRoughPlant</li>
		</tradeTags>
		<thingCategories>
			<li>PlantMatter</li>
		</thingCategories>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_RawChillshroom]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantRoughBase">
		<defName>Mashed_Ashlands_RawChillshroom</defName>
		<label>chillshroom</label>
		<description>Raw chillshroom. Often brewed into a cooling tea, not very enjoyable to eat raw.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/Mashed_Ashlands_RawChillshroom</texPath>
		</graphicData>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>40</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
		</ingestible>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_RawCorkbulbRoot]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantRoughBase">
		<defName>Mashed_Ashlands_RawCorkbulbRoot</defName>
		<label>corkbulb root</label>
		<description>Raw corkbulb root. Sometimes used as a replacement for wood when wood is not an option. Also makes for a decent animal feed, especially loved by guar and kagouti.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/Mashed_Ashlands_CorkbulbRoot</texPath>
		</graphicData>
		<stuffProps>
			<stuffAdjective>corkwood</stuffAdjective>
			<categories>
				<li>Woody</li>
			</categories>
			<commonality>0</commonality>
			<color>(184,150,104)</color>
			<constructEffect>ConstructWood</constructEffect>
			<appearance>Planks</appearance>
			<soundImpactStuff>BulletImpact_Wood</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<statFactors>
				<MaxHitPoints>0.65</MaxHitPoints>
				<Beauty>1</Beauty>
				<Flammability>1</Flammability>
				<WorkToMake>0.7</WorkToMake>
				<WorkToBuild>0.7</WorkToBuild>
				<DoorOpenSpeed>1.2</DoorOpenSpeed>
			</statFactors>
		</stuffProps>
		<statBases>
			<MarketValue>1</MarketValue>
			<StuffPower_Armor_Sharp>0.30</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.30</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.23</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>2</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>6</StuffPower_Insulation_Heat>
			<SharpDamageMultiplier>0.2</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.4</BluntDamageMultiplier>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>60</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
		</ingestible>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_RawEmperorParasolMoss]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantRoughBase">
		<defName>Mashed_Ashlands_RawEmperorParasolMoss</defName>
		<label>emperor parasol moss</label>
		<description>Moss from an emperor parasol. Not particularly tasty, but many animals seem to enjoy it. It can also be used to create an elixir that boosts resistance to ash buildup during ash storms.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/Mashed_Ashlands_EmperorParasolMoss</texPath>
		</graphicData>
		<statBases>
			<MarketValue>0.6</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>60</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<optimalityOffsetFeedingAnimals>7</optimalityOffsetFeedingAnimals>
		</ingestible>
		<allowedArchonexusCount>75</allowedArchonexusCount>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_RawMuck]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantRoughBase">
		<defName>Mashed_Ashlands_RawMuck</defName>
		<label>muck</label>
		<description>Raw muck harvested from a mucksponge. While it can be refined into several useful items, many of which are perfectly safe to ingest, raw muck itself should not be consumed.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/Mashed_Ashlands_Muck</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.3</MarketValue>
			<FoodPoisonChanceFixedHuman>1</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
			<specialThoughtDirect>Mashed_Ashlands_AteRawMuck</specialThoughtDirect>
		</ingestible>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_RawScathecraw]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantRoughBase">
		<defName>Mashed_Ashlands_RawScathecraw</defName>
		<label>scathecraw leaves</label>
		<description>Raw scathecraw leaves. Tough and chewy, not particularly pleasant to eat, though animals don't seem to mind.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/Mashed_Ashlands_RawScathecraw</texPath>
		</graphicData>
		<statBases>
			<MarketValue>0.6</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>80</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<ingestible>
			<optimalityOffsetFeedingAnimals>7</optimalityOffsetFeedingAnimals>
		</ingestible>
		<allowedArchonexusCount>75</allowedArchonexusCount>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_RawTramaRoot]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantRoughBase">
		<defName>Mashed_Ashlands_RawTramaRoot</defName>
		<label>trama root</label>
		<description>Raw trama root. Often brewed into a calming tea, not very enjoyable to eat raw.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/Mashed_Ashlands_TramaRoot</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.3</MarketValue>
		</statBases>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
		</ingestible>
	</ThingDef>
</Defs>
