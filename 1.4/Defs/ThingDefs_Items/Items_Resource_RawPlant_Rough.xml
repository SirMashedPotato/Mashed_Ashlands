<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[MorrowRim_PlantRoughBase]==========-->
	<ThingDef ParentName="RoughPlantBase" Name="MorrowRim_PlantRoughBase" Abstract="True">
		<possessionCount>10</possessionCount>
		<tradeTags>
			<li>MorrowRim_AshlandsRawPlant</li>
			<li>MorrowRim_AshlandsRawRoughPlant</li>
		</tradeTags>
	</ThingDef>
	<!-- ==========[MorrowRim_RawBittergreen]==========-->
	<ThingDef ParentName="MorrowRim_PlantRoughBase">
		<defName>MorrowRim_RawBittergreen</defName>
		<label>bittergreen petals</label>
		<description>Raw bittergreen petals. Not safe to eat raw, but perfectly safe when properly cooked. Often brewed into a fortifying tea.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/MorrowRim_BittergreenPetals</texPath>
		</graphicData>
		<statBases>
			<FoodPoisonChanceFixedHuman>0.2</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>40</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>Foods</li>
		</thingCategories>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
		</ingestible>
	</ThingDef>
	<!-- ==========[MorrowRim_RawCorkbulbRoot]==========-->
	<ThingDef ParentName="MorrowRim_PlantRoughBase">
		<defName>MorrowRim_RawCorkbulbRoot</defName>
		<label>corkbulb root</label>
		<description>Raw corkbulb root. Sometimes used as a replacement for wood when wood is not an option. Also makes for a decent animal feed, especially loved by guar and kagouti.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/MorrowRim_CorkbulbRoot</texPath>
		</graphicData>
		<stuffProps>
			<stuffAdjective>corkwood</stuffAdjective>
			<categories>
				<li>Woody</li>
			</categories>
			<commonality>0</commonality>
			<color>(184,150,104)</color>
			<constructEffect>ConstructWood</constructEffect>
			<appearance>Planks</appearance>
			<soundImpactStuff>BulletImpact_Wood</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<statFactors>
				<MaxHitPoints>0.65</MaxHitPoints>
				<Beauty>1</Beauty>
				<Flammability>1</Flammability>
				<WorkToMake>0.7</WorkToMake>
				<WorkToBuild>0.7</WorkToBuild>
				<DoorOpenSpeed>1.2</DoorOpenSpeed>
			</statFactors>
		</stuffProps>
		<statBases>
			<MarketValue>1</MarketValue>
			<StuffPower_Armor_Sharp>0.30</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Blunt>0.30</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Heat>0.23</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>2</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>6</StuffPower_Insulation_Heat>
			<SharpDamageMultiplier>0.2</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.4</BluntDamageMultiplier>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>60</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>PlantMatter</li>
		</thingCategories>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
		</ingestible>
	</ThingDef>
	<!-- ==========[MorrowRim_RawEmperorParasolMoss]==========-->
	<ThingDef ParentName="MorrowRim_PlantRoughBase">
		<defName>MorrowRim_RawEmperorParasolMoss</defName>
		<label>emperor parasol moss</label>
		<description>Moss from an emperor parasol. Not particularly tasty, but many animals seem to enjoy it. It can also be used to create an elixir that boosts resistance to ash buildup during ash storms.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/MorrowRim_EmperorParasolMoss</texPath>
		</graphicData>
		<statBases>
			<MarketValue>0.6</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>60</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>Foods</li>
		</thingCategories>
		<ingestible>
			<optimalityOffsetFeedingAnimals>7</optimalityOffsetFeedingAnimals>
		</ingestible>
		<allowedArchonexusCount>75</allowedArchonexusCount>
	</ThingDef>
	<!-- ==========[MorrowRim_RawMuck]==========-->
	<ThingDef ParentName="MorrowRim_PlantRoughBase">
		<defName>MorrowRim_RawMuck</defName>
		<label>muck</label>
		<description>Raw muck harvested from a mucksponge. While it can be refined into several useful items, many of which are perfectly safe to ingest, raw muck itself should not be consumed.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/MorrowRim_Muck</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.3</MarketValue>
			<FoodPoisonChanceFixedHuman>1</FoodPoisonChanceFixedHuman>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>30</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>PlantMatter</li>
		</thingCategories>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
			<specialThoughtDirect>MorrowRim_AteRawMuck</specialThoughtDirect>
		</ingestible>
	</ThingDef>
	<!-- ==========[MorrowRim_RawScathecraw]==========-->
	<ThingDef ParentName="MorrowRim_PlantRoughBase">
		<defName>MorrowRim_RawScathecraw</defName>
		<label>scathecraw</label>
		<description>Raw scathecraw. Tough and chewy, not particularly pleasant to eat, though animals don't seem to mind.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/MorrowRim_RawScathecraw</texPath>
		</graphicData>
		<statBases>
			<MarketValue>0.6</MarketValue>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>80</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>Foods</li>
		</thingCategories>
		<ingestible>
			<optimalityOffsetFeedingAnimals>7</optimalityOffsetFeedingAnimals>
		</ingestible>
		<allowedArchonexusCount>75</allowedArchonexusCount>
	</ThingDef>
	<!-- ==========[MorrowRim_RawTramaRoot]==========-->
	<ThingDef ParentName="MorrowRim_PlantRoughBase">
		<defName>MorrowRim_RawTramaRoot</defName>
		<label>trama root</label>
		<description>Raw trama root. Often brewed into a calming tea, not very enjoyable to eat raw.</description>
		<graphicData>
			<texPath>Things/Item/Resource/PlantFoodRaw/MorrowRim_TramaRoot</texPath>
		</graphicData>
		<statBases>
			<MarketValue>1.3</MarketValue>
		</statBases>
		<thingCategories>
			<li>PlantMatter</li>
		</thingCategories>
		<ingestible>
			<preferability>DesperateOnly</preferability>
			<canAutoSelectAsFoodForCaravan>false</canAutoSelectAsFoodForCaravan>
		</ingestible>
	</ThingDef>
</Defs>
