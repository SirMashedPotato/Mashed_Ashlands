<Defs>
	<!-- ==========[MorrowRim_InsectBase]========== -->
	<ThingDef ParentName="AnimalThingBase" Name="MorrowRim_InsectThingBase" Abstract="True">
		<modExtensions>
			<li Class="MorrowRim2.PollutionProperties">
				<AllowPollutionStimulis>false</AllowPollutionStimulis>
			</li>
		</modExtensions>
		<statBases>
			<ToxicResistance>1</ToxicResistance>
			<ToxicEnvironmentResistance MayRequire="Ludeon.RimWorld.Biotech">1</ToxicEnvironmentResistance>
			<MorrowRim_AshResistance>1</MorrowRim_AshResistance>
			<ComfyTemperatureMax>60</ComfyTemperatureMax>
			<ComfyTemperatureMin>0</ComfyTemperatureMin>
		</statBases>
		<race>
			<fleshType>Insectoid</fleshType>
			<bloodDef>Filth_BloodInsect</bloodDef>
			<useMeatFrom>Megaspider</useMeatFrom>
		</race>
		<ingredient>
			<mergeCompatibilityTags>
				<li MayRequire="Ludeon.RimWorld.Ideology">InsectMeat</li>
			</mergeCompatibilityTags>
		</ingredient>
	</ThingDef>
	<!-- ==========[MorrowRim_NixHound]========== -->
	<ThingDef ParentName="MorrowRim_InsectThingBase">
		<defName>MorrowRim_NixHound</defName>
		<label>nix-hound</label>
		<description>A rather strange insect the size of a large dog, native to ashland regions. Nix-hounds lack functioning jaws, and instead drain prey dry using their proboscis. Large packs of nix-hound can quickly take down even the greatest of warriors, leaving them nothing more than a dry husk.\n\nWhile their unconventional appearance may turn away many, nix-hounds are highly intelligent, and make for loyal companions.\n\n&lt;color=#E5E54C&gt;Draining Bite&lt;/color&gt;\nStabs from a nix-hounds proboscis do little damage, but will cause additional blood loss, nourishing the nix-hound in the process.</description>
		<statBases>
			<MoveSpeed>5.0</MoveSpeed>
			<MarketValue>350</MarketValue>
			<FilthRate>2</FilthRate>
		</statBases>
		<tools>
			<li>
				<label>left claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>10.9</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<chanceFactor>0.5</chanceFactor>
			</li>
			<li>
				<label>right claws</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>10.9</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
				<chanceFactor>0.5</chanceFactor>
			</li>
			<li>
				<label>proboscis</label>
				<capacities>
					<li>MorrowRim_DrainingStab</li>
				</capacities>
				<power>8</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
				<chanceFactor>0.9</chanceFactor>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>MorrowRim_Egg_NixHoundFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>5</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>1~2</eggCountRange>
			</li>
		</comps>
		<race>
			<body>MorrowRim_InsectDogLike</body>
			<predator>true</predator>
			<maxPreyBodySize>2.3</maxPreyBodySize>
			<petness>0.1</petness>
			<baseBodySize>0.85</baseBodySize>
			<baseHungerRate>0.18</baseHungerRate>
			<baseHealthScale>0.99</baseHealthScale>
			<foodType>CarnivoreAnimalStrict</foodType>
			<leatherDef>MorrowRim_NixHoundLeather</leatherDef>
			<nameOnTameChance>1</nameOnTameChance>
			<trainability>Advanced</trainability>
			<wildness>0.85</wildness>
			<manhunterOnTameFailChance>0.3</manhunterOnTameFailChance>
			<manhunterOnDamageChance>0.5</manhunterOnDamageChance>
			<lifeStageAges>
				<li>
					<def>AnimalBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>AnimalJuvenile</def>
					<minAge>0.2</minAge>
				</li>
				<li>
					<def>AnimalAdult</def>
					<minAge>0.45</minAge>
					<soundWounded>Pawn_Dog_Injured</soundWounded>
					<soundDeath>Pawn_Dog_Death</soundDeath>
					<soundCall>Pawn_Dog_Call</soundCall>
					<soundAngry>Pawn_Dog_Angry</soundAngry>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_SmallScratch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_SmallScratch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_SmallScratch_Miss</soundMeleeMiss>
		</race>
		<tradeTags>
			<li>AnimalUncommon</li>
			<li>AnimalFighter</li>
			<li>MorrowRim_AshlandAnimalCommon</li>
			<li>MorrowRim_AshlandAnimalFighter</li>
		</tradeTags>
	</ThingDef>
