	<!-- ==========[Mashed_Ashlands_KwamaThingBase]========== -->
	<ThingDef ParentName="Mashed_Ashlands_InsectThingBase" Name="Mashed_Ashlands_KwamaThingBase" Abstract="True">
	<statBases>
			<ComfyTemperatureMin>-50</ComfyTemperatureMin>
		</statBases>
