	<!-- ==========[MorrowRim_Mudcrab]========== -->
	<ThingDef ParentName="MorrowRim_MudcrabThingBase">
		<defName>MorrowRim_Mudcrab</defName>
		<label>ashland mudcrab</label>
		<description>A large crab often found around water, especially in swampier regions. While not particularly dangerous, unless someone mistakes it for a rock, the ashland mudcrab are is a vector for swamp fever, a disease that drains strength.</description>
		<descriptionHyperlinks>
			<HediffDef>MorrowRim_SwampFever</HediffDef>
		</descriptionHyperlinks>
		<comps>
			<li Class="CompProperties_EggLayer">
				<eggFertilizedDef>MorrowRim_Egg_MudcrabFertilized</eggFertilizedDef>
				<eggFertilizationCountMax>1</eggFertilizationCountMax>
				<eggLayIntervalDays>3</eggLayIntervalDays>
				<eggProgressUnfertilizedMax>0.5</eggProgressUnfertilizedMax>
				<eggCountRange>1~2</eggCountRange>
			</li>
		</comps>
		<tools>
			<li>
				<label>pincers</label>
				<capacities>
					<li>Bite</li>
					<li>MorrowRim_SwampBite</li>
				</capacities>
				<power>5</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Mouth</linkedBodyPartsGroup>
			</li>
		</tools>
	</ThingDef>
