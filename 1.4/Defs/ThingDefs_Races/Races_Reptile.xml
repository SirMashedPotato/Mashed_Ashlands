				<eggLayIntervalDays>1.5</eggLayIntervalDays>
				<eggCountRange>1</eggCountRange>
			</li>
		</comps>
		<race>
			<herdAnimal>true</herdAnimal>
			<body>MorrowRim_BipedalWithNoArms</body>
			<baseBodySize>0.35</baseBodySize>
			<baseHealthScale>0.6</baseHealthScale>
			<baseHungerRate>0.2</baseHungerRate>
			<foodType>OmnivoreRoughAnimal, OvivoreAnimal</foodType>
			<wildness>0.3</wildness>
			<roamMtbDays>7</roamMtbDays>
			<trainability>None</trainability>
			<lifeExpectancy>9</lifeExpectancy>
			<leatherDef>Leather_Lizard</leatherDef>
			<manhunterOnTameFailChance>0.05</manhunterOnTameFailChance>
			<!-- made by Yoann from yoann.babiesanimals -->
