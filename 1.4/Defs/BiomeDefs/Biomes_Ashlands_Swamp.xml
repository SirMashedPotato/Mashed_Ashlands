<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<BiomeDef ParentName="Mashed_Ashlands_AshlandsBase">
		<defName>Mashed_Ashlands_SwampAshlands</defName>
		<label>ashland swamp</label>
		<description>An ashland region that has become a swampy wilderness over time. Much of the land is covered by pools of stagnant water, making the landscape difficult to traverse, and especially difficult to build upon. Draggletails, violet coprinus and luminious russula can help navigating the marshes at night.\n\nFew apex predators can be found here, with nix-hounds and durzogs being the most dangerous predators one is likely to face. Of course those that frequent these swamps must also be careful around suspiciously shaped rocks, and especially careful around netch during the breeding season.</description>
		<workerClass>Mashed_Ashlands.BiomeWorker_SwampAshlands</workerClass>
		<texture>World/Biomes/Mashed_Ashlands_SwampAshlands</texture>
		<modExtensions>
			<li Class="Mashed_Ashlands.BiomeProperties">
				<lateLoader>true</lateLoader>
				<restrictPlantsToTerrain>true</restrictPlantsToTerrain>
				<increasePollutedFertility>true</increasePollutedFertility>
				<nullifyBeachTerrain>true</nullifyBeachTerrain>
				<forcedRockType>Mashed_Ashlands_Basalt</forcedRockType>
				<terrainReplacers>
					<li>
						<originalTerrain>Gravel</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_AshGravel</replacedTerrain>
					</li>
					<li>
						<originalTerrain>Sand</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_AshSand</replacedTerrain>
					</li>
				</terrainReplacers>
				<roadReplacers>
					<li>
						<originalTerrain>PackedDirt</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_PackedAsh</replacedTerrain>
					</li>
				</roadReplacers>
				<dryToReplacers>
					<li>
						<originalTerrain>MarshyTerrain</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_AshSoil</replacedTerrain>
					</li>
					<li>
						<originalTerrain>Marsh</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_AshSoil</replacedTerrain>
					</li>
					<li>
						<originalTerrain>WaterShallow</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_AshGravel</replacedTerrain>
					</li>
					<li>
						<originalTerrain>WaterOceanShallow</originalTerrain>
						<replacedTerrain>Mashed_Ashlands_AshGravel</replacedTerrain>
					</li>
				</dryToReplacers>
				<useAshlandCavePlants>true</useAshlandCavePlants>
				<useCustomCaveTerrain>true</useCustomCaveTerrain>
				<otherTerrainDef>Mashed_Ashlands_RichAshGravel</otherTerrainDef>
				<waterTerrainDef>Marsh</waterTerrainDef>
				<otherTerrainThreshold>0.25</otherTerrainThreshold>
				<waterThreshold>0.4</waterThreshold>
			</li>
		</modExtensions>
		<animalDensity>2.6</animalDensity>
		<plantDensity>0.6</plantDensity>
		<foragedFood>Mashed_Ashlands_RawMarshmerrow</foragedFood>
		<movementDifficulty>4</movementDifficulty>
		<diseases>
			<!--
			<li>
				<diseaseInc>Mashed_Ashlands_SwampFever</diseaseInc>
				<commonality>150</commonality>
			</li>
			-->
		</diseases>
		<terrainsByFertility>
			<li>
				<terrain>Mashed_Ashlands_AshSoil</terrain>
				<min>-999</min>
				<max>0.87</max>
			</li>
			<li>
				<terrain>MarshyTerrain</terrain>
				<min>0.87</min>
				<max>999</max>
			</li>
		</terrainsByFertility>
		<terrainPatchMakers>
			<li>
				<perlinFrequency>0.04</perlinFrequency>
				<thresholds>
					<li>
						<terrain>MarshyTerrain</terrain>
						<min>0.35</min>
						<max>0.65</max>
					</li>
					<li>
						<terrain>Marsh</terrain>
						<min>0.65</min>
						<max>99</max>
					</li>
				</thresholds>
			</li>
			<li>
				<perlinFrequency>0.027</perlinFrequency>
				<thresholds>
					<li>
						<terrain>Mashed_Ashlands_RichAshSoil</terrain>
						<min>-0.1</min>
						<max>0.22</max>
					</li>
					<li>
						<terrain>Mashed_Ashlands_AshMud</terrain>
						<min>0.22</min>
						<max>0.4</max>
					</li>
					<li>
						<terrain>MarshyTerrain</terrain>
						<min>0.4</min>
						<max>0.7</max>
					</li>
					<li>
						<terrain>Marsh</terrain>
						<min>0.7</min>
						<max>99</max>
					</li>
				</thresholds>
			</li>
			<li>
				<perlinFrequency>0.035</perlinFrequency>
				<thresholds>
					<li>
						<terrain>MarshyTerrain</terrain>
						<min>0.24</min>
						<max>0.48</max>
					</li>
					<li>
						<terrain>Marsh</terrain>
						<min>0.48</min>
						<max>99</max>
					</li>
				</thresholds>
			</li>
		</terrainPatchMakers>
		<baseWeatherCommonalities>
			<!-- Vanilla Weather -->
			<Clear>18</Clear>
			<Fog>6</Fog>
			<Rain>4</Rain>
			<DryThunderstorm>1</DryThunderstorm>
			<RainyThunderstorm>1</RainyThunderstorm>
			<FoggyRain>1</FoggyRain>
			<SnowGentle>4</SnowGentle>
			<SnowHard>4</SnowHard>
			<!-- MorrowRim weather -->
			<Mashed_Ashlands_Ashfall>2</Mashed_Ashlands_Ashfall>
			<Mashed_Ashlands_FoggyAshfall>1</Mashed_Ashlands_FoggyAshfall>
			<!-- Regrowth weather -->
			<RG_Drizzle MayRequire="ReGrowth.BOTR.Core">2</RG_Drizzle>
			<RG_FoggySnow MayRequire="ReGrowth.BOTR.Core">2</RG_FoggySnow>
			<RG_Overcast MayRequire="ReGrowth.BOTR.Core">6</RG_Overcast>
			<RG_Windy MayRequire="ReGrowth.BOTR.Core">8</RG_Windy>
			<RG_Blizzard MayRequire="ReGrowth.BOTR.Core">0.5</RG_Blizzard>
			<RG_Monsoon MayRequire="ReGrowth.BOTR.Core">0.5</RG_Monsoon>
		</baseWeatherCommonalities>
		<wildPlants>
			<!-- Ground covers -->
