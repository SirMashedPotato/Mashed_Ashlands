		<expandingIconDrawSize>0.8</expandingIconDrawSize>
		<neverMultiSelect>true</neverMultiSelect>
		<expandingIconColor>244,164,77</expandingIconColor>
		<texture>World/WorldObjects/Mashed_Ashlands_KwamaNest</texture>
		<comps>
			<li Class="Mashed_Ashlands.WorldObjectCompProperties_KwamaNestDetails">
				<nameNamer>Mashed_Ashlands_NamerKwamaNest</nameNamer>
