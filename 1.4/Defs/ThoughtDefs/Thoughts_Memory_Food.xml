<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_AteRawMuck]==========-->
	<ThoughtDef>
		<defName>Mashed_Ashlands_AteRawMuck</defName>
		<durationDays>1</durationDays>
		<stackLimit>1</stackLimit>
		<nullifyingGenes>
			<li MayRequire="Ludeon.RimWorld.Biotech">RobustDigestion</li>
		</nullifyingGenes>
		<stages>
			<li>
				<label>consumed muck</label>
				<description>Good thing I have a bucket right here.</description>
				<baseMoodEffect>-15</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	<!-- ==========[Mashed_Ashlands_AteNixOxMilk]==========-->
	<ThoughtDef>
		<defName>Mashed_Ashlands_AteNixOxMilk</defName>
		<durationDays>1</durationDays>
		<stackLimit>1</stackLimit>
		<nullifyingGenes>
			<li MayRequire="Ludeon.RimWorld.Biotech">RobustDigestion</li>
		</nullifyingGenes>
		<nullifyingPrecepts>
			<li MayRequire="Ludeon.RimWorld.Ideology">InsectMeatEating_Loved</li>
			<li MayRequire="vanillaexpanded.vmemese">VME_InsectMeatEating_DontCare</li>
		</nullifyingPrecepts>
		<stages>
			<li>
				<label>consumed raw nix ox mucus</label>
				<description>Bleh, tastes like old socks!</description>
				<baseMoodEffect>-3</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	<!-- ==========[Mashed_Ashlands_AteInsectEggDirect]==========-->
	<ThoughtDef>
		<defName>Mashed_Ashlands_AteInsectEggDirect</defName>
		<durationDays>1</durationDays>
		<stackLimit>1</stackLimit>
		<nullifyingPrecepts>
			<li MayRequire="Ludeon.RimWorld.Ideology">InsectMeatEating_Loved</li>
			<li MayRequire="vanillaexpanded.vmemese">VME_InsectMeatEating_DontCare</li>
		</nullifyingPrecepts>
		<stages>
			<li>
				<label>ate insect egg</label>
				<description>I ate some slimy insect eggs. It was, possibly, the most disgusting thing I've ever tasted.</description>
				<baseMoodEffect>-6</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	<!-- ==========[Mashed_Ashlands_AteInsectEggAsIngredient]==========-->
	<ThoughtDef>
		<defName>Mashed_Ashlands_AteInsectEggAsIngredient</defName>
		<durationDays>1</durationDays>
		<stackLimit>1</stackLimit>
		<nullifyingPrecepts>
			<li MayRequire="Ludeon.RimWorld.Ideology">InsectMeatEating_Loved</li>
			<li MayRequire="vanillaexpanded.vmemese">VME_InsectMeatEating_DontCare</li>
		</nullifyingPrecepts>
		<stages>
			<li>
				<label>ate cooked insect egg</label>
				<description>I ate a meal cooked with slimy insect eggs. The seasoning partly covered the taste, but there were fluid pockets in there that I can't identify.</description>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	<!-- ==========[Mashed_Ashlands_AteScuttle]==========-->
	<ThoughtDef>
		<defName>Mashed_Ashlands_AteScuttle</defName>
		<durationDays>1</durationDays>
		<stackLimit>1</stackLimit>
		<nullifyingPrecepts>
			<li MayRequire="Ludeon.RimWorld.Ideology">InsectMeatEating_Loved</li>
			<li MayRequire="vanillaexpanded.vmemese">VME_InsectMeatEating_DontCare</li>
		</nullifyingPrecepts>
		<stages>
			<li>
				<label>ate scuttle</label>
				<description>I just don't understand it.</description>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
</Defs>
