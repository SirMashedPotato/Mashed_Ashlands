<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_Plant_AshlandGrass]==========-->
	<ThingDef ParentName="Mashed_Ashlands_GrassBase" Name="Mashed_Ashlands_Plant_AshlandGrassBase">
		<defName>Mashed_Ashlands_Plant_AshlandGrass</defName>
		<label>ashland grass</label>
		<description>A species of wild grass adapted for life in the ashlands. Can grow anywhere there is a little light and ash rich ground.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<terrainAffordancesToSow>
					<li>Mashed_Ashlands_GrowAsh</li>
				</terrainAffordancesToSow>
				<disallowedTerrainForWild>
					<li>MarshyTerrain</li>
					<li>Mashed_Ashlands_RichAshSoil</li>
				</disallowedTerrainForWild>
			</li>
		</modExtensions>
		<graphicData>
			<texPath>Things/Plant/Groundcovers/Mashed_Ashlands_AshlandGrass</texPath>
		</graphicData>
		<plant>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Beginner</li>
			</sowResearchPrerequisites>
			<sowWork>170</sowWork>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_CharredAshlandGrass]==========-->
	<ThingDef ParentName="Mashed_Ashlands_Plant_AshlandGrassBase">
		<defName>Mashed_Ashlands_Plant_CharredAshlandGrass</defName>
		<label>charred ashland grass</label>
		<graphicData>
			<texPath>Things/Plant/Groundcovers/Mashed_Ashlands_CharredAshlandGrass</texPath>
		</graphicData>
		<plant>
			<maxMeshCount>4</maxMeshCount>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_AshlandDuneGrass]==========-->
	<ThingDef ParentName="Mashed_Ashlands_GrassBase">
		<defName>Mashed_Ashlands_Plant_AshlandDuneGrass</defName>
		<label>ashland dune grass</label>
		<description>A species of wild grass adapted for life in the ashlands. Can grow anywhere there is a little light and ash rich sand.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<allowedTerrainForWild>
					<li>Mashed_Ashlands_AshSand</li>
					<li>Mashed_Ashlands_AshSandVolcanic</li>
				</allowedTerrainForWild>
			</li>
		</modExtensions>
		<graphicData>
			<texPath>Things/Plant/Groundcovers/Mashed_Ashlands_AshlandDuneGrass</texPath>
		</graphicData>
		<plant>
			<sowTags Inherit="False"/>
		</plant>
		<comps>
			<li Class="Mashed_Ashlands.CompProperties_EffecterAtNight">
				<effecterDef>Mashed_Ashlands_TorchbugEffecter</effecterDef>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_AshlandFungus]==========-->
	<ThingDef ParentName="Mashed_Ashlands_SmallMushroomBase">
		<defName>Mashed_Ashlands_Plant_AshlandFungus</defName>
		<label>ashland fungus</label>
		<description>A species of wild mushroom adapted for life in the ashlands. Capable of growing in complete darkness, but needs ash rich ground to grow in.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<terrainAffordancesToSow>
					<li>Mashed_Ashlands_GrowAsh</li>
				</terrainAffordancesToSow>
				<disallowedTerrainForWild>
					<li>MarshyTerrain</li>
				</disallowedTerrainForWild>
			</li>
		</modExtensions>
		<statBases>
			<Mashed_Ashlands_AshResistance>1</Mashed_Ashlands_AshResistance>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mushrooms/Mashed_Ashlands_AshlandFungus</texPath>
		</graphicData>
		<plant>
			<growDays>2.5</growDays>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.3~0.5</visualSizeRange>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Beginner</li>
			</sowResearchPrerequisites>
			<sowWork>170</sowWork>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_Ashreed]==========-->
	<ThingDef Name="Mashed_Ashlands_Plant_AshreedBase" ParentName="Mashed_Ashlands_PlantBase">
		<defName>Mashed_Ashlands_Plant_Ashreed</defName>
		<label>ashreed</label>
		<description>A species of wild grass adapted for life in the ashlands. Often found growing near sources of water.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<requireWaterForWild>true</requireWaterForWild>
				<minTilesToWaterForWild>3</minTilesToWaterForWild>
			</li>
		</modExtensions>
		<selectable>false</selectable>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.5</Nutrition>
			<Flammability>1.0</Flammability>
			<Mashed_Ashlands_AshResistance>0.9</Mashed_Ashlands_AshResistance>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Groundcovers/Mashed_Ashlands_Ashreed</texPath>
		</graphicData>
		<hideAtSnowDepth>0.6</hideAtSnowDepth>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.3</fertilitySensitivity>
			<growDays>3</growDays>
			<harvestWork>40</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<visualSizeRange>0.8~1.2</visualSizeRange>
			<topWindExposure>0.4</topWindExposure>
			<wildClusterRadius>5</wildClusterRadius>
			<wildClusterWeight>80</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<sowTags Inherit="False"/>
		</plant>
		<comps>
			<li Class="Mashed_Ashlands.CompProperties_EffecterAtNight">
				<effecterDef>Mashed_Ashlands_TorchbugEffecter</effecterDef>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_BlueEntoloma]==========-->
	<ThingDef ParentName="Mashed_Ashlands_SmallMushroomBase">
		<defName>Mashed_Ashlands_Plant_BlueEntoloma</defName>
		<label>blue entoloma</label>
		<description>A small mushroom commonly found growing in densely forested ashland regions. Many enjoy the sight of its brilliant blue colouration.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<allowedTerrainForWild>
					<li>Mashed_Ashlands_MossyAshSoil</li>
				</allowedTerrainForWild>
				<terrainAffordancesToSow>
					<li>Mashed_Ashlands_GrowAsh</li>
				</terrainAffordancesToSow>
			</li>
		</modExtensions>
		<statBases>
			<Beauty>8</Beauty>
			<BeautyOutdoors>8</BeautyOutdoors>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mushrooms/Mashed_Ashlands_BlueEntoloma</texPath>
		</graphicData>
		<plant>
			<growDays>3</growDays>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Beginner</li>
			</sowResearchPrerequisites>
			<sowWork>200</sowWork>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_DraggleTails]==========-->
	<ThingDef Name="Mashed_Ashlands_Plant_DraggleTailsBase" ParentName="Mashed_Ashlands_PlantBase">
		<defName>Mashed_Ashlands_Plant_DraggleTails</defName>
		<label>draggle-tails</label>
		<description>A primitive flowering plant, only found growing in humid ashland regions. They emit a faint glow, often used by explorers to aide in navigating the treacherous marshes at night.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<allowedTerrainForWild>
					<li>MarshyTerrain</li>
				</allowedTerrainForWild>
			</li>
		</modExtensions>
		<selectable>false</selectable>
		<statBases>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.05</Nutrition>
			<Flammability>1.0</Flammability>
			<Mashed_Ashlands_AshResistance>0.9</Mashed_Ashlands_AshResistance>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Groundcovers/Mashed_Ashlands_DraggleTails</texPath>
		</graphicData>
		<hideAtSnowDepth>0.6</hideAtSnowDepth>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.3</fertilitySensitivity>
			<growDays>7</growDays>
			<lifespanDaysPerGrowDays>2</lifespanDaysPerGrowDays>
			<harvestWork>40</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<visualSizeRange>0.8~1.4</visualSizeRange>
			<topWindExposure>0.1</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<sowTags Inherit="False"/>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>1</glowRadius>
				<glowColor>(53,94,84,0)</glowColor>
			</li>
			<li Class="Mashed_Ashlands.CompProperties_EffecterAtNight">
				<effecterDef>Mashed_Ashlands_TorchbugEffecter</effecterDef>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_FoyadaFungus]==========-->
	<ThingDef ParentName="Mashed_Ashlands_SmallMushroomBase">
		<defName>Mashed_Ashlands_Plant_FoyadaFungus</defName>
		<label>foyada fungus</label>
		<description>A small mushroom commonly found growing in volcanically active ashland regions. Many enjoy the sight of its striking orange colouration.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<terrainAffordancesToSow>
					<li>Mashed_Ashlands_GrowAsh</li>
				</terrainAffordancesToSow>
			</li>
		</modExtensions>
		<statBases>
			<Beauty>6</Beauty>
			<BeautyOutdoors>6</BeautyOutdoors>
			<Flammability>0.2</Flammability>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mushrooms/Mashed_Ashlands_FoyadaFungus</texPath>
		</graphicData>
		<plant>
			<growDays>3</growDays>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Beginner</li>
			</sowResearchPrerequisites>
			<sowWork>200</sowWork>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_LuminousRussula]==========-->
	<ThingDef ParentName="Mashed_Ashlands_SmallMushroomBase">
		<defName>Mashed_Ashlands_Plant_LuminousRussula</defName>
		<label>luminous russula</label>
		<description>A species of small mushroom, often found growing in caves in ashland regions, or in ashland regions with high levels of humidity. Its brilliant colouration and glow is quite beautiful. Capable of growing in complete darkness, but needs ash rich ground to grow in.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<ashlandCavePlant>true</ashlandCavePlant>
				<cavePlantCommonality>0.2</cavePlantCommonality>
				<terrainAffordancesToSow>
					<li>Mashed_Ashlands_GrowAsh</li>
				</terrainAffordancesToSow>
			</li>
		</modExtensions>
		<statBases>
			<Beauty>4</Beauty>
			<BeautyOutdoors>4</BeautyOutdoors>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mushrooms/Mashed_Ashlands_LuminousRussula</texPath>
		</graphicData>
		<plant>
			<growDays>4.6</growDays>
			<visualSizeRange>0.15~0.35</visualSizeRange>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Beginner</li>
			</sowResearchPrerequisites>
			<sowWork>200</sowWork>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>1</glowRadius>
				<glowColor>(3,47,69,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_SulphurFungus]==========-->
	<ThingDef ParentName="Mashed_Ashlands_SmallMushroomBase">
		<defName>Mashed_Ashlands_Plant_SulphurFungus</defName>
		<label>sulphur fungus</label>
		<description>A small mushroom only found growing in near sulphur pools. Many enjoy the sight of its striking yellow colouration. Can only be grown on ground dense in sulphur.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<allowedTerrainForWild>
					<li>Mashed_Ashlands_SulphuricSand</li>
				</allowedTerrainForWild>
				<terrainAffordancesToSow>
					<li>Mashed_Ashlands_GrowSulphur</li>
				</terrainAffordancesToSow>
			</li>
		</modExtensions>
		<statBases>
			<Beauty>6</Beauty>
			<BeautyOutdoors>6</BeautyOutdoors>
			<Flammability>0.2</Flammability>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mushrooms/Mashed_Ashlands_SulphurFungus</texPath>
		</graphicData>
		<plant>
			<fertilityMin>0.1</fertilityMin>
			<growDays>3</growDays>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Beginner</li>
			</sowResearchPrerequisites>
			<sowWork>200</sowWork>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_Plant_VioletCoprinus]==========-->
	<ThingDef ParentName="Mashed_Ashlands_SmallMushroomBase">
		<defName>Mashed_Ashlands_Plant_VioletCoprinus</defName>
		<label>violet coprinus</label>
		<description>A species of small mushroom, often found growing in caves in ashland regions, or in ashland regions with high levels of humidity. Its brilliant colouration and glow is quite beautiful. Capable of growing in complete darkness, but needs ash rich ground to grow in.</description>
		<modExtensions>
			<li Class="Mashed_Ashlands.PlantProperties">
				<ashlandCavePlant>true</ashlandCavePlant>
				<cavePlantCommonality>0.2</cavePlantCommonality>
				<terrainAffordancesToSow>
					<li>Mashed_Ashlands_GrowAsh</li>
				</terrainAffordancesToSow>
			</li>
		</modExtensions>
		<statBases>
			<Beauty>6</Beauty>
			<BeautyOutdoors>6</BeautyOutdoors>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Mushrooms/Mashed_Ashlands_VioletCoprinus</texPath>
		</graphicData>
		<plant>
			<growDays>3.8</growDays>
			<visualSizeRange>0.2~0.4</visualSizeRange>
			<sowResearchPrerequisites>
				<li>Mashed_Ashlands_AshSowing_Beginner</li>
			</sowResearchPrerequisites>
			<sowWork>200</sowWork>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>1</glowRadius>
				<glowColor>(33,94,84,0)</glowColor>
			</li>
		</comps>
	</ThingDef>
</Defs>
