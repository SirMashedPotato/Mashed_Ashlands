<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_PlantBase]==========-->
	<ThingDef ParentName="PlantBase" Name="Mashed_Ashlands_PlantBase" Abstract="True">
		<graphicData>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<plant>
			<harvestTag>Standard</harvestTag>
			<sowTags>
				<li>Ground</li>
				<li>Mashed_Ashlands_Ash</li>
				<li>Mashed_Ashlands_AshExclusive</li>
			</sowTags>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_BushBase]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantBase" Name="Mashed_Ashlands_BushBase" Abstract="True">
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Nutrition>0.5</Nutrition>
		</statBases>
		<pathCost>30</pathCost>
		<fillPercent>0.2</fillPercent>
		<blockWind>false</blockWind>
		<ingestible>
			<foodType>Tree</foodType>
			<preferability>RawBad</preferability>
		</ingestible>
		<plant>
			<harvestWork>250</harvestWork>
			<soundHarvesting>Harvest_Tree</soundHarvesting>
			<soundHarvestFinish>Harvest_Tree_Finish</soundHarvestFinish>
			<harvestTag>Wood</harvestTag>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>10</wildClusterWeight>
			<wildOrder>2</wildOrder>
			<treeLoversCareIfChopped>false</treeLoversCareIfChopped>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_PseudoTreeBase]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantBase" Name="Mashed_Ashlands_PseudoTreeBase" Abstract="True">
		<altitudeLayer>Building</altitudeLayer>
		<statBases>
			<MaxHitPoints>195</MaxHitPoints>
			<Nutrition>0.3</Nutrition>
		</statBases>
		<passability>PassThroughOnly</passability>
		<pathCost>130</pathCost>
		<fillPercent>0.4</fillPercent>
		<blockWind>false</blockWind>
		<ingestible>
			<foodType>Tree</foodType>
			<preferability>RawBad</preferability>
		</ingestible>
		<plant>
			<sowWork>4000</sowWork>
			<blockAdjacentSow>true</blockAdjacentSow>
			<wildClusterRadius>8</wildClusterRadius>
			<wildClusterWeight>24</wildClusterWeight>
			<wildOrder>3</wildOrder>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_GrassBase]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantBase" Name="Mashed_Ashlands_GrassBase" Abstract="True">
		<selectable>false</selectable>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.45</Nutrition>
			<Flammability>1.0</Flammability>
			<Mashed_Ashlands_AshResistance>0.9</Mashed_Ashlands_AshResistance>
		</statBases>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.3</fertilitySensitivity>
			<growDays>2.5</growDays>
			<lifespanDaysPerGrowDays>3</lifespanDaysPerGrowDays>
			<leaflessGraphicPath>Things/Plant/Grass_Leafless</leaflessGraphicPath>
			<harvestWork>40</harvestWork>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.4~0.6</visualSizeRange>
			<topWindExposure>0.4</topWindExposure>
			<wildClusterRadius>5</wildClusterRadius>
			<wildClusterWeight>80</wildClusterWeight>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_SmallMushroomBase]==========-->
	<ThingDef ParentName="Mashed_Ashlands_PlantBase" Name="Mashed_Ashlands_SmallMushroomBase" Abstract="True">
		<selectable>false</selectable>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Nutrition>0.35</Nutrition>
			<Flammability>0.6</Flammability>
		</statBases>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<plant>
			<growMinGlow>-0.01</growMinGlow>
			<growOptimalGlow>1</growOptimalGlow>
			<fertilityMin>0.6</fertilityMin>
			<harvestWork>40</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<topWindExposure>0.05</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>
	<!-- ==========[Mashed_Ashlands_TreeBase]==========-->
	<ThingDef ParentName="TreeBase" Name="Mashed_Ashlands_TreeBase" Abstract="True">
		<plant>
			<mustBeWildToSow>False</mustBeWildToSow>
			<sowTags>
				<li>Ground</li>
				<li>Mashed_Ashlands_Ash</li>
				<li>Mashed_Ashlands_AshExclusive</li>
			</sowTags>
		</plant>
	</ThingDef>
</Defs>
