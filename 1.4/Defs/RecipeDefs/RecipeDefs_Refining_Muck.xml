<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_RefineMuckBase]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_RefineBase" Name="Mashed_Ashlands_RefineMuckBase" Abstract="True">
		<jobString>Refining herbal medicine from muck.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_RawMuck</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Ashlands_RawMuck</li>
			</thingDefs>
		</fixedIngredientFilter>
	</RecipeDef>
	<!-- ==========[Mashed_Ashlands_RefineMuck]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_RefineMuckBase">
		<defName>Mashed_Ashlands_RefineMuck</defName>
		<label>refine muck</label>
		<description>Refines 10 buckets of muck into 5 herbal medicine.</description>
		<products>
			<MedicineHerbal>5</MedicineHerbal>
		</products>
		<researchPrerequisite>Mashed_Ashlands_AshSurvival_Intermediate</researchPrerequisite>
	</RecipeDef>
	<!-- ==========[Mashed_Ashlands_RefineMuckWithKwamaEgg]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_RefineMuckBase">
		<defName>Mashed_Ashlands_RefineMuckWithKwamaEgg</defName>
		<label>refine muck with kwama eggs</label>
		<description>Refines 10 buckets of muck and 2 kwama eggs into 10 herbal medicine.</description>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_KwamaEgg</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Ashlands_KwamaEgg</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<MedicineHerbal>10</MedicineHerbal>
		</products>
		<researchPrerequisite>Mashed_Ashlands_AshSurvival_Advanced</researchPrerequisite>
	</RecipeDef>
</Defs>