<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_ExtractShalkChitin]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_RefineBase" Name="Mashed_Ashlands_ExtractShalkChitinBase">
		<defName>Mashed_Ashlands_ExtractShalkChitin</defName>
		<label>extract shalk resin</label>
		<description>Extracts 3 shalk resin from 10 shalk chitin.</description>
		<jobString>Extracting shalk resin.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_ShalkChitin</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Ashlands_ShalkChitin</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Mashed_Ashlands_ShalkResin>3</Mashed_Ashlands_ShalkResin>
		</products>
		<researchPrerequisite>Mashed_Ashlands_AshSurvival_Beginner</researchPrerequisite>
	</RecipeDef>
	<!-- ==========[Mashed_Ashlands_MakeWoodLogBulk]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_ExtractShalkChitinBase">
		<defName>Mashed_Ashlands_ExtractShalkChitinBulk</defName>
		<label>extract shalk resin</label>
		<description>Extracts 12 shalk resin from 40 shalk chitin.</description>
		<workAmount>1800</workAmount>
		<ingredients Inherit="False">
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_ShalkChitin</li>
					</thingDefs>
				</filter>
				<count>40</count>
			</li>
		</ingredients>
		<products Inherit="False">
			<Mashed_Ashlands_ShalkResin>12</Mashed_Ashlands_ShalkResin>
		</products>
	</RecipeDef>
</Defs>