<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_RefineNixOxMilk]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_RefineBase" Name="Mashed_Ashlands_RefineNixOxMilkBase">
		<defName>Mashed_Ashlands_RefineNixOxMilk</defName>
		<label>refine nix-ox mucous</label>
		<description>Refines 10 nix-ox mucous into 5 chemfuel.</description>
		<jobString>Refining chemfuel from nix-ox mucous.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_NixOxMilk</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Ashlands_NixOxMilk</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Chemfuel>5</Chemfuel>
		</products>
		<researchPrerequisite>Mashed_Ashlands_AshSurvival_Intermediate</researchPrerequisite>
	</RecipeDef>
	<!-- ==========[Mashed_Ashlands_RefineNixOxMilkWithShalkResin]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_RefineNixOxMilkBase">
		<defName>Mashed_Ashlands_RefineNixOxMilkWithShalkResin</defName>
		<label>refine nix-ox mucous with shalk resin</label>
		<description>Refines 10 nix-ox mucous and 5 shalk resin into 10 chemfuel.</description>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_ShalkResin</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Ashlands_ShalkResin</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products Inherit="False">
			<Chemfuel>10</Chemfuel>
		</products>
		<researchPrerequisite>Mashed_Ashlands_AshSurvival_Advanced</researchPrerequisite>
	</RecipeDef>
</Defs>