<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ESCP - Dunmer</li>
			<li>MorrowRim - Dunmer Styles</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[@Name="Mashed_Ashlands_DunmerPaperLanternTorchbug_Base"]</xpath>
					<value>
						<dominantStyleCategory MayRequire="Ludeon.RimWorld.Ideology">ESCP_DunmerStyle</dominantStyleCategory>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="Mashed_Ashlands_DunmerAshlanderLanternTorchbug_A"]</xpath>
					<value>
						<dominantStyleCategory MayRequire="Ludeon.RimWorld.Ideology">ESCP_DunmerAshlanderStyle</dominantStyleCategory>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
