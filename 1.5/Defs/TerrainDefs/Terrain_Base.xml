<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_LavaBase]==========-->
	<TerrainDef ParentName="NaturalTerrainBase" Name="Mashed_Ashlands_LavaBase" Abstract="True">
		<label>lava</label>
		<edgeType>Water</edgeType>
		<texturePath>Terrain/Surfaces/Water/Mashed_Ashlands_LavaRamp</texturePath>
		<waterDepthShader>Map/WaterDepth</waterDepthShader>
		<pollutionOverlayTexturePath IsNull="True"/>
		<renderPrecedence>300</renderPrecedence>
		<fertility>0</fertility>
		<passability>Impassable</passability>
		<pathCost>300</pathCost>
		<changeable>false</changeable>
		<avoidWander>true</avoidWander>
		<holdSnow>false</holdSnow>
		<extraDeteriorationFactor>3</extraDeteriorationFactor>
		<filthAcceptanceMask>
			<li>None</li>
		</filthAcceptanceMask>
		<tags>
			<li>Mashed_Ashlands_Lava</li>
			<li>PF_TerrainTag_Lava</li>
		</tags>
		<affordances>
			<li MayRequire="biomesteam.biomescore">BMT_TerrainAffordance_Lava</li>
		</affordances>
	</TerrainDef>
	<!-- ==========[Mashed_Ashlands_ShallowSulphuricWaterBase]==========-->
	<TerrainDef ParentName="WaterShallowBase" Name="Mashed_Ashlands_ShallowSulphuricWaterBase" Abstract="True">
		<label>shallow sulphuric water</label>
		<texturePath>Terrain/Surfaces/Water/Mashed_Ashlands_ShallowSulphuricWaterRamp</texturePath>
		<pollutedTexturePath>Terrain/Surfaces/Water/Mashed_Ashlands_ShallowSulphuricWaterRampPolluted</pollutedTexturePath>
		<waterDepthShader>Map/WaterDepth</waterDepthShader>
		<pathCost>30</pathCost>
		<renderPrecedence>394</renderPrecedence>
		<driesTo>Mashed_Ashlands_SulphuricSand</driesTo>
		<tags>
			<li>PF_TerrainTag_WaterShallow</li>
		</tags>
	</TerrainDef>
	<!-- ==========[Mashed_Ashlands_DeepSulphuricWaterBase]==========-->
	<TerrainDef ParentName="WaterDeepBase" Name="Mashed_Ashlands_DeepSulphuricWaterBase" Abstract="true">
		<label>deep sulphuric water</label>
		<texturePath>Terrain/Surfaces/Water/Mashed_Ashlands_DeepSulphuricWaterRamp</texturePath>
		<pollutedTexturePath>Terrain/Surfaces/Water/Mashed_Ashlands_DeepSulphuricWaterRampPolluted</pollutedTexturePath>
		<renderPrecedence>397</renderPrecedence>
		<tags>
			<li>PF_TerrainTag_WaterDeep</li>
		</tags>
	</TerrainDef>
</Defs>