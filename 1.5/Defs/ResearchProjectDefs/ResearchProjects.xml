	<!-- ==========[Mashed_Ashlands_AshSowing_Beginner]==========-->
	<ResearchProjectDef ParentName="Mashed_Ashlands_StarterResearchBase">
		<defName>Mashed_Ashlands_AshSowing_Beginner</defName>
		<label>beginner ashland farming</label>
		<description>Learn how to sow some of the plants native to the ashlands, an important first step for anyone that wishes to settle there.\n\nThese plants can be grown at the ash growing zone.</description>
		<researchViewX>0.00</researchViewX>
		<researchViewY>1.00</researchViewY>
		<generalRules>
			<rulesStrings>
				<li>subject->beginner ashland [action_grow]</li>
				<li>subject_story->[action_grow] [middle_grow] [thing_plant]</li>
				<li>subject_gerund->[action_grow] [thing_plant]</li>
				<!-- things -->
				<li>thing_plant->ash yam</li>
				<li>thing_plant->corkbulb</li>
				<li>thing_plant->hackle-lo</li>
				<li>thing_plant->ink-cap</li>
				<li>thing_plant->kreshweed</li>
				<li>thing_plant->parasol bush</li>
				<li>thing_plant->trama shrub</li>
				<li>thing_plant->scathecraw</li>
			</rulesStrings>
		</generalRules>
	</ResearchProjectDef>
