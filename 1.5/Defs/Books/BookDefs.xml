<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_AshlandJournal]==========-->
	<ThingDef ParentName="BookBase">
		<defName>Mashed_Ashlands_AshlandJournal</defName>
		<label>ashland journal</label>
		<description>A weathered book that seems to detail someones research on the ashlands. It's seen better days, but reading it should be able to help unlock new technologies.</description>
		<graphicData>
			<texPath>Things/Item/Book/Mashed_Ashlands_AshlandJournal/Mashed_Ashlands_AshlandJournal</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>0.8</drawSize>
		</graphicData>
		<uiIconPath>Things/Item/Book/Mashed_Ashlands_AshlandJournal/Mashed_Ashlands_AshlandJournal</uiIconPath>
		<statBases>
			<MarketValue>150</MarketValue>
		</statBases>
		<thingSetMakerTags inherit="False"/>
		<comps>
			<li Class="CompProperties_Book">
				<nameMaker>Mashed_Ashlands_Namer_AshlandJournal</nameMaker>
				<descriptionMaker>Mashed_Ashlands_Description_AshlandJournal</descriptionMaker>
				<ageYearsRange>100~200</ageYearsRange>
				<openGraphic>
					<texPath>Things/Item/Book/Mashed_Ashlands_AshlandJournal/Mashed_Ashlands_AshlandJournal_Open</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<drawSize>0.7</drawSize>
				</openGraphic>
				<verticalGraphic>
					<texPath>Things/Item/Book/Mashed_Ashlands_AshlandJournal/Mashed_Ashlands_AshlandJournal_Vertical</texPath>
					<graphicClass>Graphic_Multi</graphicClass>
					<addTopAltitudeBias>true</addTopAltitudeBias>
				</verticalGraphic>
				<doers>
					<li Class="BookOutcomeProperties_GainResearch">
						<tabs>
							<Mashed_ResearchTab_ElderScrolls/>
						</tabs>
						<exclude/>
					</li>
				</doers>
			</li>
		</comps>
	</ThingDef>
</Defs>