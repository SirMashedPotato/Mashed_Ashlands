<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_CookScribJellyPie]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_CookMealBase">
		<defName>Mashed_Ashlands_CookScribJellyPie</defName>
		<label>cook scrib jelly pie</label>
		<description>Cook a serving of scrib jelly pie using scuttle, and some vegetable.</description>
		<jobString>Cooking scrib jelly pie.</jobString>
		<workAmount>500</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_ScribJelly</li>
					</thingDefs>
				</filter>
				<count>0.4</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_RawWickwheat</li>
					</thingDefs>
				</filter>
				<count>0.4</count>
			</li>
			<li>
				<filter>
					<customSummary>extra filling</customSummary>
					<categories>
						<li>FoodRaw</li>
					</categories>
					<disallowedThingDefs>
						<li>Mashed_Ashlands_ScribJelly</li>
						<li>Mashed_Ashlands_RawWickwheat</li>
					</disallowedThingDefs>
				</filter>
				<count>0.2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Ashlands_ScribJelly</li>
				<li>Mashed_Ashlands_RawWickwheat</li>
			</thingDefs>
			<categories>
				<li>FoodRaw</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<Mashed_Ashlands_ScribJellyPie>1</Mashed_Ashlands_ScribJellyPie>
		</products>
		<researchPrerequisite>Mashed_Ashlands_AshSurvival_Advanced</researchPrerequisite>
		<skillRequirements>
			<Cooking>9</Cooking>
		</skillRequirements>
	</RecipeDef>
