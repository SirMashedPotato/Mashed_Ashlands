<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[Mashed_Ashlands_ExtractShalkChitin]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_RefineBase" Name="Mashed_Ashlands_RefineShalkChitinBase">
		<defName>Mashed_Ashlands_RefineShalkChitin</defName>
		<label>refine shalk chitin</label>
		<description>Refines 3 chemfuel from 10 shalk chitin.</description>
		<jobString>Refining chemfuel.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_ShalkChitin</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Mashed_Ashlands_ShalkChitin</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Chemfuel>3</Chemfuel>
		</products>
		<researchPrerequisite>Mashed_Ashlands_AshSurvival_Intermediate</researchPrerequisite>
	</RecipeDef>
	<!-- ==========[Mashed_Ashlands_MakeWoodLogBulk]==========-->
	<RecipeDef ParentName="Mashed_Ashlands_RefineShalkChitinBase">
		<defName>Mashed_Ashlands_RefineShalkChitinBulk</defName>
		<label>refine shalk chitin x4</label>
		<description>Refines 12 chemfuel from 40 shalk chitin.</description>
		<workAmount>1800</workAmount>
		<ingredients Inherit="False">
			<li>
				<filter>
					<thingDefs>
						<li>Mashed_Ashlands_ShalkChitin</li>
					</thingDefs>
				</filter>
				<count>40</count>
			</li>
		</ingredients>
		<products Inherit="False">
			<Chemfuel>12</Chemfuel>
		</products>
	</RecipeDef>
</Defs>